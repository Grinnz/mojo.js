{
  "name": "@mojojs/mojo",
  "version": "0.0.1",
  "description": "Real-time web framework",
  "keywords": [
    "web",
    "framework"
  ],
  "author": "Sebastian Riedel <kraih@mojolicious.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolicious/mojojs.git"
  },
  "bugs": {
    "url": "https://github.com/mojolicious/mojojs/issues"
  },
  "homepage": "https://mojojs.org",
  "scripts": {
    "lint": "eslint \"lib/**/*.js\" \"lib/*.js\" \"test/*.js\"",
    "test": "tap --no-coverage test/*.js",
    "coverage": "tap --node-arg='--no-warnings' --node-arg='--experimental-loader' --node-arg='@istanbuljs/esm-loader-hook' test/*.js",
    "coverage-html": "tap --node-arg='--no-warnings' --node-arg='--experimental-loader' --node-arg='@istanbuljs/esm-loader-hook' --coverage-report=html test/*.js"
  },
  "main": "./index.js",
  "type": "module",
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "eslint": ">=7.22.0",
    "eslint-config-semistandard": ">=15.0.1",
    "tap": ">=15.0.6"
  },
  "engines": {
    "node": ">= 15"
  },
  "dependencies": {
    "chalk": ">=4.1.1",
    "cookie": ">=0.4.1",
    "ejs": ">=3.1.6",
    "escape-string-regexp": ">=2.0.0",
    "lru-cache": ">=6.0.0",
    "nopt": ">=5.0.0",
    "stack-utils": ">=1.0.5",
    "tough-cookie": ">=4.0.0",
    "ws": ">=7.4.5"
  },
  "optionalDependencies": {
    "jsdom": ">=16.5.3"
  }
}
